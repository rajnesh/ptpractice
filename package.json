{
  "name": "bridge-bidding-sayc",
  "version": "0.2.0",
  "description": "Standard American Yellow Card (SAYC) Bridge Bidding System - Web Application",
  "main": "index.html",
  "scripts": {
    "test": "jest",
    "test:verbose": "jest --verbose",
    "test:coverage": "jest --coverage",
    "start": "npx http-server -p 8000 -o",
    "serve": "python -m http.server 8000",
    "gen-bidding-tree": "node scripts/generate_bidding_tree.js"
  },
  "keywords": [
    "bridge",
    "bidding",
    "sayc",
    "card-game",
    "contract-bridge"
  ],
  "author": "Rajnesh Kathuria",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajnesh/bridge-bidding-system.git"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0"
  },
  "directories": {
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/rajnesh/bridge-bidding-system/issues"
  },
  "homepage": "https://github.com/rajnesh/bridge-bidding-system#readme",
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/jest-setup-strip-app.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/jest-setup-strip-app.js"
    ]
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "@viz-js/lang-dot": "^1.0.5",
    "uglify-js": "^3.19.3"
  }
}